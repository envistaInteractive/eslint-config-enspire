{
    "name": "@enspire/eslint-config-enspire",
    "description": "ESLint configuration used at Enspire Commerce",
    "version": "1.0.0",
    "author": "SEE AUTHORS IN AUTHORS.md",
    "bugs": {
        "url": "https://github.com/envistaInteractive/eslint-config-enspire/issues"
    },
    "config": {
        "ghooks": {
            "pre-commit": "npm run validate"
        },
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "dependencies": {
        "eslint": "^3.16.1"
    },
    "devDependencies": {
        "commitizen": "^2.9.6",
        "cz-conventional-changelog": "^2.0.0",
        "fixpack": "^2.3.1",
        "ghooks": "^2.0.0",
        "json": "^9.0.4",
        "pre-commit": "^1.2.2"
    },
    "homepage": "https://github.com/envistaInteractive/eslint-config-enspire",
    "keywords": [
        "commerce",
        "enspire",
        "eslint",
        "eslintconfig",
        "standards",
        "styleguide"
    ],
    "license": "MIT",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/envistaInteractive/eslint-config-enspire"
    },
    "scripts": {
        "cat": "cat package.json | json -4 | tee package.json > /dev/null",
        "commit": "git-cz",
        "fix": "fixpack && npm run cat",
        "test": "eslint --debug configs"
    }
}
