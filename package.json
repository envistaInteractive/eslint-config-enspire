{
    "name": "@enspire/eslint-config-enspire",
    "description": "ESLint configuration used at Enspire Commerce",
    "version": "1.2.1",
    "author": "SEE AUTHORS IN AUTHORS.md",
    "bugs": {
        "url": "https://github.com/envistaInteractive/eslint-config-enspire/issues"
    },
    "config": {
        "ghooks": {
            "pre-commit": "npm run validate"
        },
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "dependencies": {
        "eslint": "^8.10.0"
    },
    "devDependencies": {
        "chai": "^4.3.6",
        "chai-string": "^1.5.0",
        "commitizen": "^4.2.4",
        "cz-conventional-changelog": "^3.3.0",
        "fixpack": "^4.0.0",
        "ghooks": "^2.0.4",
        "json": "^11.0.0",
        "pre-commit": "^1.2.2"
    },
    "engines": {
        "node": ">=6.9.1"
    },
    "homepage": "https://github.com/envistaInteractive/eslint-config-enspire",
    "keywords": [
        "commerce",
        "enspire",
        "envista",
        "eslint",
        "eslintconfig",
        "standards",
        "styleguide"
    ],
    "license": "MIT",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/envistaInteractive/eslint-config-enspire"
    },
    "scripts": {
        "commit": "git-cz",
        "fix": "fixpack",
        "test": "eslint configs tests",
        "test:debug": "eslint --debug configs tests",
        "validate": "npm run fix && npm run test"
    }
}
